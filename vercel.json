{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/api/index.py" },
    { "src": "/chat", "dest": "/api/index.py" },
    { "src": "/image", "dest": "/api/index.py" },
    { "src": "/api/(.*)", "dest": "/api/index.py" }
  ]
}
